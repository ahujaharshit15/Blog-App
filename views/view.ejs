<%- include("partials/header") -%>


    <% post.map(function(onePost){ %>
        <div class="card">
            <div class="card-body">
                <form action="/deleteOne" method="POST">
                <h1 class="card-title">
                    <%= onePost.title %>
                </h1>
                <p class="card-text">
                    <%= onePost.details %>
                </p>
            
                <button name=deleteID value= <%= onePost.id %> type="submit" class="btn btn-danger">Delete</button>
            </form>
            </div>
            
        </div>
        <br>



        <% }) %>

     



            <%- include("partials/footer") -%>